
<!DOCTYPE html>
<html lang="tr">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @RenderSection("metatags", required: false)
    <meta property="og:locale" content="tr_TR" />
    <meta property="og:type" content="website" />
    <title>@Html.Raw(ViewBag.Title)</title>
    <meta name="description" content="@Html.Raw(ViewBag.Description)" />
    <meta name="keywords" content="@Html.Raw(ViewBag.Keyword)" />
    <meta property="og:title" content="@Html.Raw(ViewBag.Title)" />
    <meta property="og:description" content="@Html.Raw(ViewBag.Description)" />
    <meta property="og:image" content="/Content/Yeni/Logo/logo=1.png" />
    <meta property="og:url" content="@Request.Url" />
    <meta property="twitter:card" content="@Html.Raw(ViewBag.Title)" />
    <meta name="twitter:title" content="@Html.Raw(ViewBag.Keyword)" />
    <meta name="twitter:description" content="@Html.Raw(ViewBag.Description)" />
    <meta property="twitter:url" content="@Request.Url" />
    <meta name="author" content="Mekasoftech">
    <link rel="canonical" href="@Request.Url">
    <link rel="alternate" hreflang="tr" href="@Request.Url" />
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href='https://fonts.googleapis.com/css?family=Lato:100' rel='stylesheet' type='text/css'>
    <link rel="preload" href="~/Content/Yeni/video/moon.mp4" as="video" type="video/mp4">
    <!-- custom styles (optional) -->
    <link href="~/Content/Yeni/assets/css/plugins.css" rel="stylesheet" />
    <link href="~/Content/Yeni/assets/css/style.css" rel="stylesheet" />
    <meta name="generator" content="Mekasoftech" />
    <link rel="alternate" type="application/json+oembed" href="~/Content/Orjinal/wp-json/oembed/1.0/embed79e6.json" />
    <link rel="alternate" type="text/xml+oembed" href="~/Content/Orjinal/wp-json/oembed/1.0/embed4971" />
    <link rel="icon" href="~/Content/Yeni/Logo/logo-4.png" sizes="32x32" />
    <link rel="icon" href="~/Content/Yeni/Logo/logo-4.png" sizes="192x192" />
    <link rel="apple-touch-icon-precomposed" href="~/Content/Yeni/Logo/logo-4.png" />
    <meta name="msapplication-TileImage" content="~/Content/Yeni/Logo/logo-4.png" />
    @*<script language="javascript" src="http://ir.sitekodlari.com/sagtusengelleme1.js"></script>*@
