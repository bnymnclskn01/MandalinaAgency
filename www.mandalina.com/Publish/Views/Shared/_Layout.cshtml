</head>
<body class="v-light dsn-effect-scroll dsn-cursor-effect dsn-ajax classic-menu" @*onselectstart="return false"*@>

    <!-- ========== Menu ========== -->
    <div class="site-header dsn-load-animate dsn-container">
        <div class="extend-container d-flex w-100 align-items-baseline justify-content-between align-items-end">
            <div class="inner-header p-relative">
                <div class="main-logo">
                    <a href="/" data-dsn="parallax">
                        <img class="light-logo"
                             src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw=="
                             data-dsn-src="/Content/Yeni/Logo/logo=1.png" alt="" />
                        <img class="dark-logo"
                             src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw=="
                             data-dsn-src="/Content/Yeni/Logo/logo-3.png" alt="" />
                    </a>
                </div>
            </div>
            <div class="menu-icon d-flex align-items-baseline">
                <div class="text-menu p-relative  font-heading text-transform-upper">
                    <div class="p-absolute text-button">Menü</div>
                    <div class="p-absolute text-open">Aç</div>
                    <div class="p-absolute text-close">Kapalı</div>
                </div>
                <div class="icon-m" data-dsn="parallax" data-dsn-move="10">
                    <span class="menu-icon-line p-relative d-inline-block icon-top"></span>
                    <span class="menu-icon-line p-relative d-inline-block icon-center"></span>
                    <span class="menu-icon-line p-relative d-block icon-bottom"></span>
                </div>
            </div>
            <nav class="accent-menu dsn-container main-navigation p-absolute  w-100  d-flex align-items-baseline ">
                <div class="menu-cover-title">Menu</div>
                <ul class="extend-container p-relative d-flex flex-column justify-content-center h-100">
                    <li>
                        <a href="/">

                            <span class="dsn-title-menu">Anasayfa</span>
                            <span class="dsn-meta-menu">01</span>
                            <span class="dsn-bg-arrow"></span>
                        </a>
                    </li>

                    <li>
                        <a href="/hakkimizda">

                            <span class="dsn-title-menu">Hakkımızda</span>
                            <span class="dsn-meta-menu">02</span>
                            <span class="dsn-bg-arrow"></span>
                        </a>
                    </li>

                    <li>
                        <a href="/portfolyo">

                            <span class="dsn-title-menu">Portfolyo</span>
                            <span class="dsn-meta-menu">03</span>
                            <span class="dsn-bg-arrow"></span>
                        </a>
                    </li>

                    <li>
                        <a href="/iletisim" class="user-no-selection">

                            <span class="dsn-title-menu">İletişim</span>
                            <span class="dsn-meta-menu">04</span>
                            <span class="dsn-bg-arrow"></span>
                        </a>
                    </li>
                </ul>
            </nav>
        </div>
    </div>
    <!-- ========== End Menu ========== -->

    <main class="main-root">
        <div id="dsn-scrollbar">
            @RenderBody()
            <footer class="footer  background-section">
                <div class="dsn-container ">
                    <div class="row">
                        <div class="col-lg-12 footer-block text-center mt-30 border-top pt-30">
                            <div class="footer-block-inner">
                                <div class="nav-footer">
                                    <ul>
                                        <li class="d-inline-block over-hidden">
                                            <a class="link-hover" data-dsn="parallax"
                                               href="/" data-hover-text="Home">ANASAYFA</a>
                                        </li>
                                        <li class="d-inline-block over-hidden">
                                            <a class="link-hover" data-dsn="parallax"
                                               href="/hakkimizda" data-hover-text="Portfolio">HAKKIMIZDA</a>
                                        </li>
                                        <li class="d-inline-block over-hidden">
                                            <a class="link-hover" data-dsn="parallax"
                                               href="/portfolyo" data-hover-text="About">PORTFOLYO</a>
                                        </li>
                                        <li class="d-inline-block over-hidden">
                                            <a class="link-hover" data-dsn="parallax"
                                               href="/iletisim" data-hover-text="Contact">İLETİŞİM</a>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-12 fotter-block mt-15 text-center border-top pt-30">
                            <div class="footer-block-inner">
                                <div class="nav-footer">
                                    <ul>
                                        @{ Html.RenderAction("PartialSocialMediaList", "Home"); }
                                    </ul>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-12 fotter-block  mt-15">
                            <div class="footer-block-inner">
                                <div class="copyright">
                                    <div class="text-center">
                                        <div class="copright-text over-hidden">
                                            © @DateTime.Now.Year Mandalina Ajans Designed by  <a class="link-hover" data-hover-text="Meka Softech" href="https://instagram.com/mekasoftech"
                                                                                                 target="_blank">
                                                Meka Softech
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </footer>

        </div>
    </main>


    <!-- ========== Cursor Page ========== -->
    <!-- ========== End Cursor Page ========== -->
    <!-- ========== Light & Dark Options ========== -->
    <div class="day-night">
        <div class="night active" data-dsn-theme="dark">
